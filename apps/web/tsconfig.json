{
  "extends": "tsconfig/nextjs.json",
  "exclude": [
    "node_modules",
    "generated/*"
  ],
  "compilerOptions": {
    "paths": {
      "@core/*": [
        "./core/*"
      ],
      "@packages/*": [
        "./packages/*"
      ],
      "@components/*": [
        "./components/*"
      ]
    }
  }
}